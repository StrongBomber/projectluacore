local TextChatService = game:GetService("TextChatService")
local channel = TextChatService.TextChannels.RBXGeneral

task.spawn(function()
	while true do
		pcall(function()
			channel:SendAsync("/team fuck uuuu") -- Mesajı sen istersen değiştir
		end)
		task.wait() -- Maksimum hızda çalışır
	end
end)
