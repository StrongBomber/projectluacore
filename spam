local TextChatService = game:GetService("TextChatService")
local channel = TextChatService.TextChannels.RBXGeneral

task.spawn(function()
	while true do
		pcall(function()
			channel:SendAsync("TEAM RAIWESY - THE BEST") -- Mesajı sen istersen değiştir
		end)
		task.wait(2) -- Maksimum hızda çalışır
	end
end)
