-- Scripts:

local function SNINDAT_fake_script() -- AvatarImage.LocalScript 
	local script = Instance.new('LocalScript', AvatarImage)

	local imageLabel = script.Parent
	
	
	local playerId = game.Players.LocalPlayer.UserId
	local imageUrl = "https://www.roblox.com/headshot-thumbnail/image?userId=" .. tostring(playerId) .. "&width=420&height=420&format=png"
	
	
	imageLabel.Image = imageUrl
	
end
coroutine.wrap(SNINDAT_fake_script)()
local function EKCR_fake_script() -- PlayerName.LocalScript 
	local script = Instance.new('LocalScript', PlayerName)

	local label = script.Parent
	
	label.Text = game.Players.LocalPlayer.Name
	
end
coroutine.wrap(EKCR_fake_script)()
local function LNTQMR_fake_script() -- RejoinButton.LocalScript 
	local script = Instance.new('LocalScript', RejoinButton)

	script.Parent.MouseButton1Click:Connect(function()
	
		local TeleportService = game:GetService("TeleportService")
	
		-- Oyuna yeniden katılmak için fonksiyon
		local function RejoinGame()
			local success, errorMessage = pcall(function()
				local gameId = game.GameId
				if gameId then
					TeleportService:TeleportToGameInstance(gameId, game.Players.LocalPlayer)
				else
					warn("Oyun ID'si bulunamadı.")
				end
			end)
	
			if not success then
				warn("Yeniden katılma başarısız: " .. errorMessage)
			end
		end
	
		-- Belirli bir süre sonra otomatik olarak yeniden katılma
		wait(1) -- Örnek olarak, 10 saniye sonra yeniden katılma yapılacak
		RejoinGame()
	
	
	end)
end
coroutine.wrap(LNTQMR_fake_script)()
local function ZJHXVFD_fake_script() -- TaxiBossAutoFarmButton.LocalScript 
	local script = Instance.new('LocalScript', TaxiBossAutoFarmButton)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/StrongBomber/projectluacore/main/taxiboss.lua')()
	end)
end
coroutine.wrap(ZJHXVFD_fake_script)()
local function RQRVENF_fake_script() -- MinimizeButton.LocalScript 
	local script = Instance.new('LocalScript', MinimizeButton)

	local Frame = script.Parent.Parent.Parent.MainFrame
	local OpenB = script.Parent.Parent.Parent.OpenButton
	
	
	script.Parent.MouseButton1Click:Connect(function()
		Frame.Visible = false
		OpenB.Visible = true
	end)
end
coroutine.wrap(RQRVENF_fake_script)()
local function LBTTK_fake_script() -- CloseButton.LocalScript 
	local script = Instance.new('LocalScript', CloseButton)

	local Frame = script.Parent.Parent.Parent.MainFrame
	
	
	script.Parent.MouseButton1Click:Connect(function()
		Frame:Destroy()
	end)
end
coroutine.wrap(LBTTK_fake_script)()
local function WPKMX_fake_script() -- OpenButton.LocalScript 
	local script = Instance.new('LocalScript', OpenButton)

	local Frame = script.Parent.Parent.MainFrame
	local OpenB = script.Parent
	
	script.Parent.MouseButton1Click:Connect(function()
		Frame.Visible = true
		OpenB.Visible = false
	end)
end
coroutine.wrap(WPKMX_fake_script)()
